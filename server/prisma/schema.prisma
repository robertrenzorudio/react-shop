// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./inventory.db"
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id             String @id @default(uuid())
  name           String @unique
  manufacturer   String
  price          Int
  formattedPrice String
  image          String
}
